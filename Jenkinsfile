node{
    stage("Build"){
        sh "cd /root"
        sh "ls -l"
        sh "whoami"
        sh "pwd"
        println("Build")
    }
    stage("Build docker images"){
        println("Build docker images")
    }
    stage("Push Docker Registry"){
        println("Pushing")
    }
    stage("Deploy"){
        println("Deploying")
    }
}